{
  "name": "add-to-calendar-button-vue",
  "version": "1.2.0",
  "engines": {
    "node": ">=14.20.0"
  },
  "description": "Convenient Vue.js wrapper for the add to calendar button snippet, which lets you reliably create beautiful buttons, where people can add events to their calendars.",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/atcb.umd.cjs",
  "module": "./dist/atcb.js",
  "types": "./dist/AddToCalendarButtonComponent.vue.d.ts",
  "exports": {
    ".": {
      "import": "./dist/atcb.js",
      "require": "./dist/atcb.umd.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/add2cal/add-to-calendar-button-vue.git"
  },
  "keywords": [
    "vue",
    "javascript",
    "css",
    "microsoft",
    "apple",
    "google",
    "widget",
    "generator",
    "component",
    "calendar",
    "event",
    "free",
    "snippet",
    "outlook",
    "add-to-calendar",
    "add-to-calendar-button",
    "add-event"
  ],
  "author": {
    "name": "Jens Kuerschner",
    "url": "https://jenskuerschner.de/"
  },
  "license": "SEE LICENSE.txt",
  "bugs": {
    "url": "https://github.com/add2cal/add-to-calendar-button/issues"
  },
  "homepage": "https://add-to-calendar-button.com/",
  "scripts": {
    "eslint": "npx eslint .",
    "eslint:fix": "npm run eslint -- --fix",
    "prettier": "npx prettier . --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run eslint:fix && npm run prettier:fix",
    "test": "vitest",
    "serve": "vite preview",
    "build": "vite build && vue-tsc --emitDeclarationOnly"
  },
  "dependencies": {
    "add-to-calendar-button": "~1.18.5"
  },
  "devDependencies": {
    "@testing-library/vue": "^6.6.1",
    "@types/jest": "^29.2.1",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-commonjs": "^1.0.2",
    "eslint-plugin-security": "^1.5.0",
    "jsdom": "^20.0.2",
    "prettier": "2.7.1",
    "typescript": "^4.8.4",
    "vite": "^3.2.2",
    "vitest": "^0.24.5",
    "vue": "^3.2.41",
    "vue-tsc": "^1.0.9"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  }
}
